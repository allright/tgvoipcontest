FROM debian:10.1
RUN apt-get update -y && \
    apt-get clean && \
    apt-get install -y \
    unzip python2.7 python-pip \
    && apt-get clean

RUN pip install requests


RUN mkdir /work
WORKDIR /work
COPY submission.zip ./
COPY testdata ./
RUN ls -al

RUN unzip submission.zip
WORKDIR submission

RUN pwd
RUN ls -al
RUN ./src/tgvoipcallpy/tgvoipcallpy.py
RUN ./tgvoiprate ../in.wav ../out.wav


